<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1">
    <link rel="stylesheet" type="text/css" href="~/assets/css/bootstrap-clearmin.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/roboto.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/material-design.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/small-n-flat.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/css/custom.css">
    <title>@ViewBag.Title -Todos los derechos reservados</title>
</head>
<body class="cm-no-transition cm-1-navbar">
    <div id="cm-menu">
        <nav class="cm-navbar cm-navbar-primary">
            <div class="cm-flex">
                <a href="index.html" class="cm-logo">Censo<span>Academico</span></a>
            </div>
            <div class="btn btn-primary md-menu-white" data-toggle="cm-menu"></div>
        </nav>
        <div id="cm-menu-content">
            <div id="cm-menu-items-wrapper">
                <div id="cm-menu-scroller">
                    <ul class="cm-menu-items">
                        <li class="active">
                        <a href="/" class="sf-house">Inicio</a>
                        </li>           
                        <li>
                        <a href="/Asignaturas" class="sf-folder-document">Asignaturas</a>
                        </li>
                        <li>
                            <a href="/Horario" class="sf-calendar">Horario</a>
                        </li>
                        <li>
                            <a href="/Periodos" class="sf-file-bookmark">Periodos</a>
                        </li>
                        <li>
                            <a href="/Alumno" class="sf-user-male">Alumno</a>
                        </li>
                        <li>
                            <a href="/CensoDetalle" class="sf-institution">CensoDetalle</a>
                        </li>
                        <li>
                            <a href="/CensoMaster" class="sf-institution">Censo</a>
                        </li>
</ul>
                </div>
            </div>
        </div>
    </div>
    <header id="cm-header">
        <nav class="cm-navbar cm-navbar-primary">
            <div class="btn btn-primary md-menu-white hidden-md hidden-lg" data-toggle="cm-menu"></div>
            <div class="cm-flex">
                <h1></h1>
                <form id="cm-search" action="index.html" method="get">
                    <input type="search" name="q" autocomplete="off" placeholder="Search...">
                </form>
            </div>
            <div class="dropdown pull-right">
                <button class="btn btn-primary md-account-circle-white" data-toggle="dropdown"></button>
                <ul class="dropdown-menu">
                    <li>
                        <a href="/Login"><i class="fa fa-fw fa-sign-out"></i> Salir</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div id="global">
        <div class="container-fluid">
            <div class="panel panel-default">
                <div class="panel-body" id="demo-buttons">
                    @RenderBody()
                </div>
            </div>
        </div>
        <footer class="cm-footer">
            <span class="pull-left">
                <p>&copy; @DateTime.Now.Year - Censo Informatica Administrativa</p>
            </span>
        </footer>
    </div>
    <script src="~/assets/js/lib/jquery-2.1.3.min.js"></script>
    <script src="~/assets/js/jquery.mousewheel.min.js"></script>
    <script src="~/assets/js/jquery.cookie.min.js"></script>
    <script src="~/assets/js/fastclick.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/clearmin.min.js"></script>
    <script src="~/assets/js/demo/home.js"></script>
</body>
</html>
